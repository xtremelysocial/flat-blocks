{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "image-border",
	"title": "Image Border",
	"blockTypes": [ "core/gallery", "jetpack/tiled-gallery" ],
	"styles": {
		"blocks": {
			"core/image": {
				"border": {
					"style": "solid",
					"width": "1px",
					"color": "var(--wp--custom--color--border)",
					"radius": "var(--wp--custom--border--radius)"
				}
			}
		}
	}	
}
