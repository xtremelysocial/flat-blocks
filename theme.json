		"layout": {
			"contentSize": "760px",
			"wideSize": "1100px"
		},
		"useRootPaddingAwareAlignments": true,
		"appearanceTools": true,
		"spacing": {
			"blockGap": true,
			"padding": true,
			"margin": true,
			"units": ["%", "px", "em", "rem", "vh", "vw"],
            "spacingScale": {
                "steps": 0
            },
			"spacingSizes": [
				{
					"slug": "20",
					"size": "calc(0.25 * var(--wp--custom--spacing--gap))",
					"name": "2X-Small"
				},
				{
					"slug": "30",
					"size": "calc(0.5 * var(--wp--custom--spacing--gap))",
					"name": "X-Small"
				},
				{
					"slug": "40",
					"size": "clamp(var(--wp--preset--spacing--30), 3vw, var(--wp--custom--spacing--gap))",
					"name": "Small (Responsive X-Small->Small)"
				},
				{
					"slug": "50",
					"size": "clamp(var(--wp--preset--spacing--40), 4vw, calc(1.5 * var(--wp--custom--spacing--gap)))",
					"name": "Medium (Responsive Small->Medium)"
				},
				{
					"slug": "60",
					"size": "calc(2 * var(--wp--preset--spacing--40)) /*calc(var(--wp--custom--spacing--gap) + var(--wp--preset--spacing--40))*/",
					"name": "Large (Responsive 2 x Small)"
				},
				{
					"slug": "70",
					"size": "clamp(var(--wp--preset--spacing--60), 6vw, calc(3 * var(--wp--custom--spacing--gap)))",
					"name": "X-Large (Responsive Large->X-Large)"
				},
				{
					"slug": "80",
					"size": "calc(2 * var(--wp--preset--spacing--60))",
					"name": "2X-Large (Responsive X-Large->2X-Large)"
				}
			]
		},
